{
  "name": "sfynx-project/tool-utils-bundle",
  "type": "symfony-bundle",
  "description": "This Bundle provides tool libraries and services of route and role and twig extensions.",
  "keywords": ["route", "role", "soap", "file", "regex", "date", "encryption", "curl", "random"],
  "homepage": "http://pigroupe.github.io/cmf-sfynx/",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Etienne de Longeaux",
      "email": "etienne.delongeaux@gmail.com"
    },
    {
      "name": "Sfynx Community",
      "homepage": "https://github.com/pigroupe/SfynxToolBundle/contributors"
    }
  ],

  "require": {
    "symfony/framework-bundle": "~2.8|~3.0",
    "twig/extensions": "~1.4.0"
  },

  "require-dev": {
    "symfony/phpunit-bridge": "~2.7|~3.0",
    "phpunit/phpunit": "*"
  },

  "suggest": {
  },

  "conflict": {
  },

  "autoload": {
    "psr-0": { "Sfynx\\ToolBundle": "" }
  },

  "target-dir": "Sfynx/ToolBundle",

  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  }
}
